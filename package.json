{
  "name": "nomad-second-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec babel-node -r dotenv/config src/bin/www",
    "dev": "nodemon --exec babel-node -r dotenv/config src/bin/www",
    "clean": "rm -rf dist",
    "build": "yarn clean && babel ./src --out-dir dist --copy-files",
    "prod": "yarn build && node -r dotenv/config ./dist/bin/www"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "puppeteer": "^23.7.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.35.1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^8.1.1",
    "node-cron": "^3.0.3",
    "regenerator-runtime": "^0.14.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-plugin-module-resolver": "^5.0.0",
    "nodemon": "^2.0.15"
  }
}

